<?xml version="1.0" encoding="UTF-8"?>
<svg width="384" height="384" viewBox="0 0 384 384" xmlns="http://www.w3.org/2000/svg">
  <!-- Fond doré avec dégradé -->
  <defs>
    <linearGradient id="goldGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#D4AF6A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#B39562;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur in="SourceAlpha" stdDeviation="3"/>
      <feOffset dx="0" dy="2" result="offsetblur"/>
      <feComponentTransfer>
        <feFuncA type="linear" slope="0.3"/>
      </feComponentTransfer>
      <feMerge>
        <feMergeNode/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>

  <!-- Fond principal -->
  <rect width="384" height="384" rx="57.599999999999994" fill="url(#goldGradient)"/>

  <!-- Bordure subtile -->
  <rect x="19.200000000000003" y="19.200000000000003" width="345.6" height="345.6"
        rx="46.08" fill="none" stroke="#FFFFFF" stroke-width="3.84" opacity="0.3"/>

  <!-- Texte DPM -->
  <text x="50%" y="50%"
        font-family="Arial, Helvetica, sans-serif"
        font-size="134.39999999999998"
        font-weight="bold"
        fill="#FFFFFF"
        text-anchor="middle"
        dominant-baseline="central"
        filter="url(#shadow)">
    DPM
  </text>

  <!-- Accent décoratif -->
  <circle cx="326.4" cy="57.599999999999994" r="19.200000000000003" fill="#FFFFFF" opacity="0.4"/>
  <circle cx="57.599999999999994" cy="326.4" r="11.52" fill="#FFFFFF" opacity="0.3"/>
</svg>