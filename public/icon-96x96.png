<?xml version="1.0" encoding="UTF-8"?>
<svg width="96" height="96" viewBox="0 0 96 96" xmlns="http://www.w3.org/2000/svg">
  <!-- Fond doré avec dégradé -->
  <defs>
    <linearGradient id="goldGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#D4AF6A;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#B39562;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow" x="-50%" y="-50%" width="200%" height="200%">
      <feGaussianBlur in="SourceAlpha" stdDeviation="3"/>
      <feOffset dx="0" dy="2" result="offsetblur"/>
      <feComponentTransfer>
        <feFuncA type="linear" slope="0.3"/>
      </feComponentTransfer>
      <feMerge>
        <feMergeNode/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>

  <!-- Fond principal -->
  <rect width="96" height="96" rx="14.399999999999999" fill="url(#goldGradient)"/>

  <!-- Bordure subtile -->
  <rect x="4.800000000000001" y="4.800000000000001" width="86.4" height="86.4"
        rx="11.52" fill="none" stroke="#FFFFFF" stroke-width="0.96" opacity="0.3"/>

  <!-- Texte DPM -->
  <text x="50%" y="50%"
        font-family="Arial, Helvetica, sans-serif"
        font-size="33.599999999999994"
        font-weight="bold"
        fill="#FFFFFF"
        text-anchor="middle"
        dominant-baseline="central"
        filter="url(#shadow)">
    DPM
  </text>

  <!-- Accent décoratif -->
  <circle cx="81.6" cy="14.399999999999999" r="4.800000000000001" fill="#FFFFFF" opacity="0.4"/>
  <circle cx="14.399999999999999" cy="81.6" r="2.88" fill="#FFFFFF" opacity="0.3"/>
</svg>